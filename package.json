    "uport-connect": "^0.7.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-pug": "^5.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "cli-glob": "^0.1.0",
    "es-collections": "^0.0.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "gh-pages": "^1.1.0",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-front-matter": "^1.3.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-sym": "0.0.14",
    "jsdoc": "3.4.2",
    "minimatch": "^2.0.10",
    "minimist": "^1.2.0",
    "prettier": "^1.11.1",
    "remark-cli": "^4.0.0",
    "remark-preset-lint-recommended": "^3.0.1",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^17.0.0",
    "write-good": "^0.11.3"
  },
  "keywords": [
    "uport",
    "ethereum",
    "ipfs",
    "identity"
  ],
  "license": "Apache 2.0",
  "main": "n/a",
  "scripts": {
    "clean:repos": "rm -rf repos/*",
    "watch:repos": "gulp watch:repos",
    "fetch": "gulp git:clone",
    "copy:markdown": "gulp copy-markdown",
    "install:plugins": "cd ./plugins/gatsby-remark-copy-linked-files && npm install && cd ../gatsby-remark-catch-relative-markdown-links && npm install && cd ../../",
    "update:remote:docs": "npm run clean:repos && npm run fetch && npm run copy:markdown",
    "setup": "npm run install:plugins && npm run clean && npm run update:remote:docs",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve & ngrok http 9000 && kill $!",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public .cache",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public,static .",
    "lint:md": "remark content/public/",
    "write-good": "write-good $(glob 'content/public/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
